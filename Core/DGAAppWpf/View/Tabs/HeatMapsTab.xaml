<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DGA.Views"
             xmlns:messages="clr-namespace:DGA.UIElements.Messages"
             xmlns:syncfusion="http://schemas.syncfusion.com/wpf" x:Class="DGA.Views.HeatMapsTab"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1920">
    <Grid Background="#FFE5E5E5">
        <Label Content="HeatMaps" HorizontalAlignment="Stretch" VerticalAlignment="Top" FontSize="25" Background="DarkOliveGreen" Foreground="Black" Grid.ColumnSpan="5"/>
        <messages:DeactivatedMessage x:Name="deactivatedMsg"  Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Grid.ColumnSpan="5" Grid.RowSpan="4" Panel.ZIndex="2" />

    </Grid>
</UserControl>
